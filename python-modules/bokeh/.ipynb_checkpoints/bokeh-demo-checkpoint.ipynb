   "source": [
    "#basic bokeh graph\n",
    "\n",
    "from bokeh.plotting import figure\n",
    "from bokeh.io import output_file, show"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 13,
   "metadata": {},
   "outputs": [],
   "source": [
    "x = [1, 2, 3, 4, 5]\n",
    "y = [6, 7, 8, 9, 10]\n",
    "\n",
    "#prepare output file, figure, and plot\n",
    "output_file('demo.html')\n",
    "f = figure()\n",
    "f.line(x, y)\n",
    "f.triangle(x, y)\n",
    "f.circle(x, y)\n",
    "show(f)"
   ]
  },
